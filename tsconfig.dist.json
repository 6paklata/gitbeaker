{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "target": "es6",
    "strict": false,
    "strictPropertyInitialization": false,
    "strictNullChecks": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
  ]
}
